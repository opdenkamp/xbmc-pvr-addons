msgctxt "#30000"
msgid "Server Hostname or IP"
msgstr "Nombre o IP del Servidor"

msgctxt "#30001"
msgid "Keep IP address to 127.0.0.1, if server is running on this same system"
msgstr "Utiliza la IP 127.0.0.1 si el servidor esta corriendo en esta misma máquina."

msgctxt "#30002"
msgid "Port Number"
msgstr "Puerto"

msgctxt "#30003"
msgid "Stream timeout, unknown problem"
