msgid "New and rerun"
msgstr "Nové časti a reprízy"

msgctxt "#30105"
msgid "New only"
msgstr "Iba nové časti"

msgctxt "#30106"
msgid "Live only"
msgstr "Iba živé vysielanie"

msgctxt "#30107"
msgid "Channel:"
msgstr "Kanál:"

msgctxt "#30108"
msgid "Any Channel"
msgstr "Akýkoľvek kanál"

msgctxt "#30110"
msgid "Airtime:"
msgstr "Čas vysielania:"

