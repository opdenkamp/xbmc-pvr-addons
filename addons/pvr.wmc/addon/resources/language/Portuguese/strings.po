msgctxt "#30000"
msgid "Server Hostname or IP"
msgstr "Hostname ou IP do Servidor"

msgctxt "#30001"
msgid "Keep IP address to 127.0.0.1, if server is running on this same system"
msgstr "Usar o endereço IP 127.0.0.1, se o servidor está presente neste mesmo sistema"

msgctxt "#30002"
msgid "Port Number"
msgstr "Número da Porta"

msgctxt "#30003"
msgid "Stream timeout, unknown problem"
msgstr "O tempo limite da transmissão esgotou, problema desconhecido"

msgctxt "#30004"
