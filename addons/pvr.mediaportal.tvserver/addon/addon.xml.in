    library_osx="XBMC_Mediaportal.pvr"
    library_freebsd="XBMC_Mediaportal.pvr"
    library_wingl="XBMC_Mediaportal_win32.dll"
    library_windx="XBMC_Mediaportal_win32.dll"
    library_android="libXBMC_Mediaportal.so"/>
