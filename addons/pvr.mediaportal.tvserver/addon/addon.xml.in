<addon
  id="pvr.mediaportal.tvserver"
  version="1.9.12"
  name="MediaPortal PVR Client"
  provider-name="Marcel Groothuis">
  <requires>
    <c-pluff version="0.1"/>
    <import addon="xbmc.pvr" version="1.9.0"/>
